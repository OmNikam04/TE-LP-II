<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>a2</title>
    <link rel="stylesheet" href="../static/css/bootstrap.min.css">
    <script src='../static/js/bootstrap.min.js'></script>
</head>
<body>
    <div class="container">
        <form action="/api/songs" method="get">
            <button type="submit" class="btn">submit</button>
        </form>
    </div>
    <br/>
    <a href="/">home</a>
    <% if (songs != null) { %>
        <table class="table">
            <thead>
                <tr>
                    <th>song</th>
                    <th>Music Director </th>
                    <th>Singer</th>
                    <th>Movie </th>
                </tr>
            </thead>

            <tbody>
                <% songs.forEach(function(item) { %>
                    <tr>
                        <td><%= item.songName %></td>
                        <td><%= item.musicDirector %></td>
                        <td><%= item.singer %></td>
                        <td><%= item.film %></td>
                    </tr>
                <% }); %>
            </tbody>
        </table>
    <% } %>
</body>
</html>
